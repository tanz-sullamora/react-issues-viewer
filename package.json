{
  "name": "react-issue-viewer",
  "version": "0.0.1",
  "description": "github issues viewer",
  "scripts": {
    "start": "npm run serve && npm run watch",
    "watch": "cross-env NODE_ENV=development webpack --watch --progress",
    "serve": "cross-env NODE_ENV=development webpack-dev-server --hot --host localhost --port 8080",
    "build": "cross-env NODE_ENV=production webpack",
    "clean": "rimraf dist",
    "test": "cross-env NODE_ENV=tests karma start --single-run || true",
    "coverage": "cross-env NODE_ENV=coverage karma start --single-run || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tanz-sullamora/react-issues-viewer.git"
  },
  "author": "Artem Shavrin <tanz@sullamora.ru>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tanz-sullamora/react-issues-viewer/issues"
  },
  "homepage": "https://github.com/tanz-sullamora/react-issues-viewer#readme",
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "jasmine": "^2.5.2",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-es6-shim": "^1.0.0",
    "karma-html-reporter": "^0.2.7",
    "karma-jasmine": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "moxios": "^0.3.0",
    "postcss-focus": "^1.0.0",
    "postcss-loader": "^0.13.0",
    "precss": "^1.4.0",
    "react-addons-test-utils": "^15.3.2",
    "redux-mock-store": "^1.2.1",
    "rimraf": "^2.5.4",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1",
    "webpack-fail-plugin": "^1.0.5"
  },
  "dependencies": {
    "axios": "^0.14.0",
    "cross-env": "^3.0.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "redux": "^3.6.0"
  }
}
